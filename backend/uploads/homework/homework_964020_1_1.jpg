<svg width="800" height="600" viewBox="0 0 800 600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <!-- 模糊滤镜 -->
    <filter id="blur" x="-20%" y="-20%" width="140%" height="140%">
      <feMorphology operator="dilate" radius="1"/>
      <feGaussianBlur stdDeviation="2,1"/>
    </filter>
  </defs>
  
  <!-- 背景 -->
  <rect width="800" height="600" fill="#f8f8f8"/>
  
  <!-- 模糊的文字内容 -->
  <g filter="url(#blur)">
    <text x="400" y="50" text-anchor="middle" font-family="SimSun" font-size="20" fill="#666">数学作业</text>
    <text x="50" y="120" font-family="SimSun" font-size="14" fill="#888">1. ??? + ??? = ?</text>
    <text x="50" y="180" font-family="SimSun" font-size="14" fill="#888">2. ??? - ??? = ?</text>
    <text x="50" y="240" font-family="SimSun" font-size="14" fill="#888">3. ??? × ??? = ?</text>
    
    <!-- 更模糊的答案 -->
    <text x="200" y="120" font-family="SimSun" font-size="12" fill="#aaa">??</text>
    <text x="200" y="180" font-family="SimSun" font-size="12" fill="#aaa">??</text>
    <text x="200" y="240" font-family="SimSun" font-size="12" fill="#aaa">??</text>
  </g>
  
  <!-- 添加噪点 -->
  <circle cx="150" cy="200" r="2" fill="#ddd"/>
  <circle cx="350" cy="150" r="1" fill="#ddd"/>
  <circle cx="500" cy="300" r="2" fill="#ddd"/>
  
  <text x="50" y="500" font-family="SimSun" font-size="10" fill="#999">图片质量较差，文字模糊</text>
</svg>